{
  "version": "0.5.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "A react component for DHIS2 custom apps which monitors network connection to DHIS2 server",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "install:test-tools": "npm install $(npm pack ../test-tools | tail -1) --force",
    "publish": "npm publish --access=public"
  },
  "peerDependencies": {
    "@testing-library/react": "^11.2.5",
    "notistack": "^1.0.3",
    "react": "^17.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@dhis2-app/network-check",
  "author": "jakub-bao",
  "module": "dist/network-check.esm.js",
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "husky": "^4.3.8",
    "react": "*",
    "react-dom": "^17.0.1",
    "size-limit": "^4.9.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@dhis2-app/test-tools": "file:dhis2-app-test-tools-0.1.0.tgz"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/dhis2-app/network-check.git"
  }
}
